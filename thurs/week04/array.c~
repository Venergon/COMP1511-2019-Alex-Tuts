// Some testing with arrays

#include <stdio.h>
#include <stdlib.h>

#define ARRAY_SIZE 20

int main(void) {
    int squares[ARRAY_SIZE] = {0};
    
    int sum = 0;
    int i = 0;
    while (i < ARRAY_SIZE) {
        squares[i] = i * i;
        sum += squares[i];
        i++;
    }
    
    
    
    
    return EXIT_SUCCESS;
}
